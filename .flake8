[flake8]
max-line-length = 88
extend-ignore = E203,W503
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    .tox,
    venv,
    .venv,
    env,
    .env,
    migrations,
    node_modules,
    staticfiles,
    media,
    logs,
    archive,
    backups,
    htmlcov
per-file-ignores =
    */tests/*:F401,E501
    *settings*.py:F401,F403,F405
    __init__.py:F401
select = 
    E,
    W,
    F,
    C,
    B,
    I